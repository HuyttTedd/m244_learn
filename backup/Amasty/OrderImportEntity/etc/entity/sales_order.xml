<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:amasty:module:Amasty_ImportCore:etc/am_import.xsd">
    <entity code="sales_order">
        <name>Order</name>
        <group>Sales</group>
        <description>Order Import Entity</description>
        <fieldsConfig>
            <fieldsClass class="Amasty\ImportCore\Import\FieldsClass\Describe">
                <arguments>
                    <argument name="tableName" xsi:type="string">sales_order</argument>
                </arguments>
            </fieldsClass>
            <rowValidation class="Amasty\OrderImportEntity\Import\Validation\RowValidator\OrderRowValidator">
                <excludeBehaviors>
                    <deleteDirect/>
                    <updateDirect/>
                    <addUpdateDirect/>
                </excludeBehaviors>
            </rowValidation>
            <fields>
                <field name="entity_id">
                    <actions>
                        <trim apply="beforeValidate"/>
                    </actions>
                    <validation>
                        <nonNegative/>
                        <notEmpty>
                            <excludeBehaviors>
                                <addDirect/>
                            </excludeBehaviors>
                        </notEmpty>
                    </validation>
                    <identifier>
                        <label>Order Entity ID</label>
                    </identifier>
                </field>
                <field name="state">
                    <validation>
                        <notEmpty/>
                        <custom class="Amasty\OrderImportEntity\Import\Validation\EntityValidator\State" error="The specified %1 doesn't exist."/>
                    </validation>
                    <filter>
                        <type>select</type>
                        <options>
                            <class>Amasty\OrderImportEntity\Import\SourceOption\Order\OrderState</class>
                        </options>
                    </filter>
                    <required/>
                </field>
                <field name="status">
                    <validation>
                        <notEmpty/>
                        <custom class="Amasty\OrderImportEntity\Import\Validation\EntityValidator\Status" error="The specified %1 doesn't exist."/>
                    </validation>
                    <filter>
                        <type>select</type>
                        <options>
                            <class>Amasty\OrderImportEntity\Import\SourceOption\Order\OrderStatus</class>
                        </options>
                    </filter>
                    <required/>
                </field>
                <field name="is_virtual">
                    <validation>
                        <boolean/>
                    </validation>
                    <filter>
                        <type>toggle</type>
                    </filter>
                    <required/>
                </field>
                <field name="store_id">
                    <validation>
                        <storeExists/>
                    </validation>
                    <filter>
                        <type>store</type>
                    </filter>
                    <required/>
                </field>
                <field name="customer_id">
                    <validation>
                        <customerExists/>
                    </validation>
                    <required/>
                </field>
                <field name="customer_group_id">
                    <validation>
                        <customerGroupExists/>
                    </validation>
                    <filter>
                        <type>select</type>
                        <options>
                            <class>Amasty\ImportCore\Import\SourceOption\CustomerGroupOptions</class>
                        </options>
                    </filter>
                    <required/>
                </field>
                <field name="order_currency_code">
                    <filter>
                        <type>select</type>
                        <options>
                            <class>Amasty\OrderImportEntity\Import\SourceOption\Order\CurrencyOptions</class>
                        </options>
                    </filter>
                </field>
                <field name="customer_firstname">
                    <required/>
                </field>
                <field name="customer_lastname">
                    <required/>
                </field>
                <field name="base_grand_total">
                    <required/>
                </field>
                <field name="base_shipping_amount">
                    <required/>
                </field>
                <field name="base_subtotal">
                    <required/>
                </field>
                <field name="base_tax_amount">
                    <required/>
                </field>
                <field name="grand_total">
                    <required/>
                </field>
                <field name="shipping_amount">
                    <required/>
                </field>
                <field name="subtotal">
                    <required/>
                </field>
                <field name="tax_amount">
                    <required/>
                </field>
                <field name="base_to_global_rate">
                    <required/>
                </field>
                <field name="base_to_order_rate">
                    <required/>
                </field>
                <field name="total_qty_ordered">
                    <required/>
                </field>
                <field name="base_total_refunded">
                    <actions>
                        <defaultValue value="0"/>
                    </actions>
                </field>
                <field name="total_refunded">
                    <actions>
                        <defaultValue value="0"/>
                    </actions>
                </field>
                <field name="shipping_address_id">
                    <required/>
                </field>
                <field name="billing_address_id">
                    <required/>
                </field>
                <field name="quote_id">
                    <required/>
                </field>
                <field name="increment_id">
                    <required/>
                    <identifier>
                        <label>Order Increment ID</label>
                    </identifier>
                </field>
                <field name="customer_email">
                    <validation>
                        <notEmpty/>
                    </validation>
                    <required/>
                </field>
                <field name="can_ship_partially">
                    <validation>
                        <boolean/>
                    </validation>
                    <filter>
                        <type>toggle</type>
                    </filter>
                </field>
                <field name="can_ship_partially_item">
                    <validation>
                        <boolean/>
                    </validation>
                    <filter>
                        <type>toggle</type>
                    </filter>
                </field>
                <field name="customer_is_guest">
                    <validation>
                        <boolean/>
                    </validation>
                    <filter>
                        <type>toggle</type>
                    </filter>
                </field>
                <field name="customer_note_notify">
                    <validation>
                        <boolean/>
                    </validation>
                    <filter>
                        <type>toggle</type>
                    </filter>
                </field>
                <field name="customer_dob">
                    <validation>
                        <dateFormat/>
                    </validation>
                </field>
                <field name="created_at">
                    <validation>
                        <dateFormat/>
                    </validation>
                    <required/>
                </field>
                <field name="updated_at">
                    <validation>
                        <dateFormat/>
                    </validation>
                    <required/>
                </field>
                <field name="adjustment_negative">
                    <actions>
                        <defaultValue value="0"/>
                    </actions>
                </field>
                <field name="shipping_method">
                    <validation>
                        <custom class="Amasty\OrderImportEntity\Import\Validation\EntityValidator\ShippingMethod" error="The specified %1 doesn't exist."/>
                    </validation>
                    <filter>
                        <type>select</type>
                        <options>
                            <class>Magento\Shipping\Model\Config\Source\Allmethods</class>
                        </options>
                    </filter>
                </field>
            </fields>
