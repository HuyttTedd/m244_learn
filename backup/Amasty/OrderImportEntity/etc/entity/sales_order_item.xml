    <entity code="sales_order_item">
        <name>Order Item</name>
        <group>Sales</group>
        <description>Order Item Import Entity</description>
        <fieldsConfig>
            <fieldsClass class="Amasty\ImportCore\Import\FieldsClass\Describe">
                <arguments>
                    <argument name="tableName" xsi:type="string">sales_order_item</argument>
                </arguments>
            </fieldsClass>
            <fields>
                <field name="item_id">
                    <actions>
                        <trim apply="beforeValidate"/>
                    </actions>
                    <validation>
                        <nonNegative/>
                        <notEmpty>
                            <excludeBehaviors>
                                <addDirect/>
                            </excludeBehaviors>
                        </notEmpty>
                    </validation>
                    <required/>
                    <synchronization>
                        <item>
                            <entityName>sales_invoice_item</entityName>
                            <fieldName>order_item_id</fieldName>
                        </item>
                        <item>
                            <entityName>sales_creditmemo_item</entityName>
                            <fieldName>order_item_id</fieldName>
                        </item>
                        <item>
                            <entityName>sales_shipment_item</entityName>
                            <fieldName>order_item_id</fieldName>
                        </item>
                    </synchronization>
                </field>
                <field name="parent_item_id">
                    <required/>
                </field>
                <field name="order_id">
                    <actions>
                        <trim apply="beforeValidate"/>
                    </actions>
                    <validation>
                        <notEmpty/>
                        <nonNegative/>
                        <tableRowExists tableName="sales_order"
                                        idFieldName="entity_id"
                                        rootOnly="true"
                                        error="Order entity with specified 'entity_id' doesn't exist">
                            <excludeBehaviors>
                                <deleteDirect/>
                            </excludeBehaviors>
                        </tableRowExists>
                    </validation>
                    <required/>
                </field>
                <field name="store_id">
                    <validation>
                        <storeExists/>
                    </validation>
                    <filter>
                        <type>store</type>
                    </filter>
                    <required/>
                </field>
                <field name="product_id">
                    <required/>
                </field>
                <field name="product_type">
                    <validation>
                        <notEmpty>
                            <includeBehaviors>
                                <addDirect/>
                                <addUpdateDirect/>
                            </includeBehaviors>
                        </notEmpty>
                    </validation>
                    <filter>
                        <type>select</type>
                        <options>
                            <class>Amasty\OrderImportEntity\Import\SourceOption\OrderItem\ProductTypesOptions</class>
                        </options>
                    </filter>
                    <required/>
                </field>
                <field name="no_discount">
                    <actions>
                        <defaultValue value="0"/>
                    </actions>
                </field>
                <field name="weight">
                    <required/>
                </field>
                <field name="is_virtual">
                    <validation>
                        <boolean/>
                    </validation>
                    <filter>
                        <type>toggle</type>
                    </filter>
                    <required/>
                </field>
                <field name="sku">
                    <required/>
                </field>
                <field name="name">
                    <required/>
                </field>
                <field name="is_qty_decimal">
                    <validation>
                        <boolean/>
                    </validation>
                    <filter>
                        <type>toggle</type>
                    </filter>
                </field>
                <field name="qty_ordered">
                    <validation>
                        <greaterThanZero/>
                    </validation>
                    <required/>
                </field>
                <field name="price">
                    <actions>
                        <defaultValue value="0"/>
                    </actions>
                    <required/>
                </field>
                <field name="base_price">
                    <actions>
                        <defaultValue value="0"/>
                    </actions>
                    <required/>
                </field>
                <field name="row_total">
                    <actions>
                        <defaultValue value="0"/>
